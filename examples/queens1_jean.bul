#const n = 9.

dom[1..n]. 

dom[Y] => q(X,Y) : dom[X]. 
dom[X] => q(X,Y) : dom[Y]. 

% row and column 
dom[X1], dom[X2], dom[Y], X1 < X2 => ~q(X1,Y), ~q(X2,Y). 
dom[X], dom[Y1], dom[Y2], Y1 < Y2 => ~q(X,Y1), ~q(X,Y2). 

% diagonals
dom[X1], dom[X2], dom[Y1], dom[Y2], X1 < X2, Y1 >= Y2, X1 + Y1 == X2 + Y2 => 
    ~q(X1,Y1), ~q(X2,Y2), . 
dom[X1], dom[X2], dom[Y1], dom[Y2], X1 < X2, Y1 <= Y2, X1 - Y1 == X2 - Y2 => 
    ~q(X1,Y1), ~q(X2,Y2), . 
